{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "docs",
      "databaseName": "<DB name>",
      "collectionName": "<Collection name>",
      "connectionStringSetting": "DB_DOCUMENTDB"
    }
  ]
}